<div class="formulario container col-md-4">
  <h1>Edit Attraction</h1>
    <%= simple_form_for @attraction do |f| %>
      <%= f.input :name %>
      <%= f.input :address, placeholder: "Street/Country" %>
      <%= f.input :description %>
      <%= f.input :text, as: :text %>
      <%= f.input :photos, as: :file, input_html: { multiple: true } %>
      <%= f.button :submit,"Add attraction", class: "btn btn-outline-secondary" %>
  <% end %>
</div>
